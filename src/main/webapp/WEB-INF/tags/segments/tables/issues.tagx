<jsp:root
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:spring="http://www.springframework.org/tags"
	xmlns:form="http://www.springframework.org/tags/form"
	xmlns:shept-pager="http://www.shept.org/tags/shept-pager"
	xmlns:shept-opt="http://www.shept.org/tags/shept-opt"
	xmlns:shept-core="http://www.shept.org/tags/shept-core"
	version="2.0"
>

	<shept-core:table > 
	  <jsp:attribute name="tabHeader" >
		<tr class="sheptTableHeader" >
			<th/>
			<th nowrap="nowrap">
				<shept-pager:sort sortCol="name" code="issue" pageHolder="${commandPath}" />	
			</th>
			<th/>
			<th/>
			<th/>
			<th/>
			<th/>
		</tr>
	  </jsp:attribute>
	  
	  <jsp:attribute name="tabRow" >
		<tr class="${rowClassDefault}">
			<td>
				<shept-opt:iconRow  />
			</td>
			<td>
				<shept-opt:blurInput path="name" />
			</td>
			<td>
				${rowObject.level}
			</td>
			<td>
				<form:input path="datecreated" disabled="true" cssErrorClass="sheptFieldError" size="8"/>
			</td>
			<td>
				<c:choose>
					<c:when test="${not isTransient}">
						<shept-opt:chainRow chainName="childIssues" code="issues" image="16x16/script_go.png" />
					</c:when>
					<c:otherwise>
						<img src="${images}/16x16/blank.png" alt="" />
					</c:otherwise>
				</c:choose>
			</td>
			<td>
				<shept-opt:confirmDeleteRow />
			</td>
		</tr>
	  </jsp:attribute>

	  <jsp:attribute name="tabSubmit" >
	  	<shept-opt:submission />
	  </jsp:attribute>
	</shept-core:table>

</jsp:root>